{
  "name": "test-drive-development",
  "version": "0.1.0",
  "private": true,
  "jest": {
    "setupFilesAfterEnv": [
      "<rootDir>/setupTests.js"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.4",
    "@emotion/styled": "^11.11.5",
    "@mui/material": "^5.15.21",
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/jsonpath": "^0.2.4",
    "@types/node": "^16.18.101",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "axios": "^1.7.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-scripts": "5.0.1",
    "ts-node": "^10.9.2",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "jest",
    "eject": "react-scripts eject",
    "e2e": "cypress run",
    "watch:test": "chokidar 'src/**/*.{js,jsx,ts,tsx}' 'cypress/e2e/**/*.{js,jsx,ts,tsx}' -c 'npx cypress run'",
    "watch:server": "json-server --watch ./stub-server/db.json --port 8080"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.24.7",
    "@babel/preset-env": "^7.24.7",
    "@babel/preset-typescript": "^7.24.7",
    "babel-jest": "^29.7.0",
    "chokidar-cli": "^3.0.0",
    "cypress": "^13.12.0",
    "cypress-watch-and-reload": "^1.10.15",
    "eslint-plugin-cypress": "^3.3.0",
    "jest": "^29.7.0",
    "json-server": "^1.0.0-beta.1",
    "jsonpath": "^1.1.1"
  }
}